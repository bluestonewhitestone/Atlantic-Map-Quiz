function newFunctionOne(){
let img = document.createElement("img");
img.src = "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRbs8Kg6C1zcXYsDaEXp1pNODQERMvkUuCKMA&usqp=CAU";

